<body>
<div id="body">
   <h1>Add New Grocery Store</h1>

   <form id="addNewStore" action="/" method="POST">
       <input type="text" name="store" placeholder="Store Name"/>
       <input type="text" name="city" placeholder="City"/>
       <input id="postBtn" type="submit" value="Post"/>
   </form>
</div>
<div>

 {{#stores}}
    <ul>
        <li>
            <a href="/store/{{id}}">{{store}}</a>
                <p>{{city}}</p>

        </li>
    </ul>
 {{/stores}}

</div>
</body>
